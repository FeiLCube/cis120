import java.util.*;

public class Coordinate {

	private int x, y;
	
	public Coordinate(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	public int getX() {
		return this.x;
	}
	
	public int getY() {
		return this.y;
	}
	
	public Map<Integer, Integer> getCoordinate() {
		Map<Integer, Integer> coor = new TreeMap<Integer, Integer>();
		coor.put(x, y);
		return coor;
	}
}
